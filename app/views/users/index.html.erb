<%= display_date_and_email%>
<%= render "user_drop_down"%>
<table>
	<tr>
	<td>DP</td>
	<td>Name</td>
	<td>Email</td>
	</tr>
	
	<% @user.each do |p|%>
	<tr>
		<td><%= image_tag p.image.photo(:thumb) if !p.image.blank?%></td>
		<td><%= p.name%></td>
		<td><%= p.email%></td>
	</tr>
		<%end%>
	
</table>
<%= will_paginate @user %>