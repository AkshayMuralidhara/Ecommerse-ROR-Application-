<%= render "error_message"%>
<table>
<%= form_for @user do |f| %>
<tr>
<td><%= f.label :name%></td>
<td><%= f.text_field :name%></td>
</tr>
<tr>
<td><%= f.label :email%></td>
<td><%= f.text_field :email%></td>
</tr>
<tr>
<td><%= f.label :password%></td>
<td><%= f.password_field :password%></td>
</tr>
<tr>
	<td><%= f.submit%></td>	
</tr>
<%end%>